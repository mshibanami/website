{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Next: Node",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/next",
            "port": 9229,
            "env": {
                "NODE_OPTIONS": "--inspect"
            },
            "sourceMapPathOverrides": {
                "webpack:///*": "${webRoot}/server/*"
            },
            "resolveSourceMapLocations": [
                "${workspaceFolder}/**",
                "!**/node_modules/**"
            ]
        }
    ],
}
